:EMMO_e1028129-c23e-57ac-9174-2f34ddbf3926 a owl:Class ;
    :EMMO_405ae501-8898-59c0-b2db-0afa09d561f9 "https://en.wikipedia.org/wiki/Maximum_energy_product"^^rdf:PlainLiteral ;
    rdfs:subClassOf [ a owl:Restriction ;
